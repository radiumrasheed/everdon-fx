end_of_line=crlf
insert_final_newline=true
indent_style=tab
indent_size=2

[{*.ng,*.sht,*.html,*.shtm,*.shtml,*.htm}]
indent_style=tab
tab_width=2

[{.babelrc,.stylelintrc,jest.config,.eslintrc,.prettierrc,*.json,*.jsb3,*.jsb2,*.bowerrc}]
indent_style=tab
tab_width=2

[*.css]
indent_style=tab
tab_width=2

[{.sequelizerc,*.js}]
indent_style=tab
tab_width=2

[{*.ats,*.ts}]
indent_style=tab
tab_width=2

[{tsconfig.app.json,tsconfig.e2e.json,tsconfig.json,tsconfig.spec.json}]
indent_style=tab
tab_width=2

[*.js.map]
indent_style=tab
tab_width=2

[*.less]
indent_style=space
indent_size=2

[*.scss]
indent_style=tab
tab_width=2

[{*.cjsx,*.coffee}]
indent_style=space
indent_size=2

[{.analysis_options,*.yml,*.yaml}]
